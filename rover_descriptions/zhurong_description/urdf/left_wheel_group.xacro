				<origin xyz="0 0 0"
								rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://zhurong_description/meshes/leftMainRockerF_Link.STL" />
				</geometry>
			</collision> -->

			<visual>
				<origin xyz="0 0 0"
								rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://zhurong_description/meshes/leftMainRockerF_Link.STL" />
				</geometry>
				<material name="">
					<color rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
				</material>
			</visual>

			<inertial>
				<origin xyz="0.374406440378762 0.093415363997234 -7.12779188987767E-05"
								rpy="0 0 0"/>
				<mass value="${suspension_arm_F_L_Mass}"/>
				<inertia
						ixx="0.0449363534239293"
						ixy="-0.127290840096917"
						ixz="4.62980586570407E-05"
						iyy="0.527745441781329"
