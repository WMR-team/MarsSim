<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

	<xacro:macro name="right_wheel_tree" params="">
		<link name='suspension_arm_F_R'>
			<!-- <collision>
					<origin xyz="0 0 0"
									rpy="0 0 0"/>
					<geometry>
							<mesh filename="package://zhurong_description/meshes/rightMainRockerF_Link.STL" />
					</geometry>
			</collision> -->

			<visual>
				<origin xyz="0 0 0"
								rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://zhurong_description/meshes/rightMainRockerF_Link.STL" />
				</geometry>
				<material name="">
					<color rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
				</material>
			</visual>

			<inertial>
				<origin xyz="0.374406441189298 -0.0934153639972333 -6.89062960095574E-05"
								rpy="0 0 0"/>
				<mass value="${suspension_arm_F_L_Mass}"/>
				<inertia
						ixx="0.044936"
						ixy="0.12729"
						ixz="5.1728E-05"
						iyy="0.52775"
						iyz="-2.5537E-05"
						izz="0.5667" />
			</inertial>
		</link>

		<joint name="suspension_arm_F_R_joint" type="revolute">
			<parent link="chassis"/>
			<child link="suspension_arm_F_R"/>
			<origin xyz="0 -0.425 0" rpy="0 0 0"/>
			<limit lower="${-suspension_arm_F_L_limit}" upper="${suspension_arm_F_L_limit}" effort="${suspension_effort}" velocity="${suspension_speed}"/>
			<axis xyz="0 1 0"/>
			<joint_properties damping="${suspension_damping}" friction="${suspension_friction}"/>
			<!-- <safety_controller soft_upper="2" soft_lower="1" k_position="1" k_velocity="2" /> -->
		</joint>

		<transmission name="suspension_arm_F_R_trans">
			<type>transmission_interface/SimpleTransmission</type>
			<joint name="suspension_arm_F_R_joint">
				<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
			</joint>
			<actuator name="suspension_arm_F_RMotor">
				<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
				<mechanicalReduction>1</mechanicalReduction>
			</actuator>
		</transmission>

		<link name='suspension_arm_B_R'>
